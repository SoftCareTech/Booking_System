{
  "expo": {
    "name": "Booking_System",
    "slug": "Booking_System",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ico_lox.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
